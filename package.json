{
	"name": "daily-tarot",
	"displayName": "Daily Tarot",
	"description": "Get your daily tarot reading - Prisma Vision",
	"version": "0.1.0",
	"publisher": "raptazure",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tarot.today",
		"onCommand:tarot.ask"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "tarot.today",
				"title": "Daily Tarot: Draw a Card"
			},
			{
				"command": "tarot.ask",
				"title": "Daily Tarot: Ask a Question"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}